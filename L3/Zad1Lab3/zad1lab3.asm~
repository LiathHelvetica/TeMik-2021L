LAST_INDEX SET 00011001b

ORG 0900h
DATA:	DB 11000000b
	DB 11111001b
	DB 10100100b
	DB 10110000b
	DB 10011001b
	DB 10010010b
	DB 10000010b
	DB 11111000b
	DB 10000000b
	DB 10010000b
	DB 10001000b
	DB 10000011b
	DB 10100111b
	DB 11000110b
	DB 10100001b
	DB 10000110b
	DB 10001110b
	DB 10010000b
	DB 10001011b
	DB 10001001b
	DB 11000111b
	DB 10100011b
	DB 10001100b
	DB 11100011b
	DB 11000001b
	DB 11111111b

ORG 0000h
	MOV DPTR, #DATA
MAIN:	MOV A, P2
	CPL A
	LJMP READ
FIN:	MOV A, R0
	MOVC A, @A+DPTR
	MOV P3, A
	LJMP MAIN

READ:	MOV R0, A
	SUBB A, #LAST_INDEX
	JC FIN
	MOV R0, #LAST_INDEX
	LJMP FIN

END